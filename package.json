{
	"name": "socialide",
	"description": "A Social IDE",
	"version": "0.0.1",
	"preview": true,
	"displayName": "INAHASHA",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:users",
		"onView:chat"
	],
	"main": "./out/main.js",
	"contributes": {
		
		"viewsContainers": {
			"activitybar": [
				{
					"id": "test-id",
					"title": "Socaila IDE",
					"icon": "resources/computer.svg"
				}
			]
		},
		"views": {
			"test-id": [
				{
					"id": "chat",
					"name": "Chat"
				}
			]
		},
		"commands": [
			{
				"command": "chat.send_to_all",
				"title": "Send",
				"icon":{
					"dark": "resources/send_dark.svg",
					"light": "resources/send_light.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "chat.send_to_all",
					"when": "view == chat",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"scaledrone-node": "^1.5.0"
	}
}